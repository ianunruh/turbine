<h2><%= team.name %> &mdash; <%= service.name %></h2>

<table class="table">
<thead>
  <tr>
    <th>Timestamp</th>
    <th>Elapsed</th>
    <th>Output</th>
  </tr>
</thead>
<tbody>
<% for(var i = 0; i < check_results.length; i++) { %>
  <%
    var clazz;
    if (check_results[i].passed) {
      clazz = 'success';
    } else {
      clazz = 'danger';
    }
  %>
  <tr class="<%= clazz %>">
    <td class="nowrap">
      <span class="since"><%= check_results[i].checked_at %></span>
    </td>
    <td class="nowrap">
      <%= check_results[i].elapsed.toFixed(2) %>s
    </td>
    <td>
      <% if (!check_results[i].passed) { %>
        <pre><%= check_results[i].output %></pre>
      <% } %>
    </td>
  </tr>
<% } %>
</tbody>
</table>
